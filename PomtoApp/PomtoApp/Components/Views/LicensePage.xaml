<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PomtoApp.Components.Pages"
             x:Class="PomtoApp.Views.LicensePage"
             NavigationPage.HasNavigationBar="False">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/license.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#license" ComponentType="{x:Type local:LicenseComponent}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
    
</ContentPage>