<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PomtoApp.Components.Pages"
             x:Class="PomtoApp.Views.NewAccountPage"
             NavigationPage.HasNavigationBar="False">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/new-account.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#newaccount" ComponentType="{x:Type local:NewAccountUserComponent}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentPage>